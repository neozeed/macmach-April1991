includes::
	$(MKDIRHIER) $(BUILDINCDIR)/.
	@(set -x; cd $(BUILDINCDIR)/.; for i in $(HEADERS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)/./$(CURRENT_DIR)/$$i .; \
	done)

install:: $(HEADERS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(INCDIR)); \
	done

install:: XErrorDB
	$(INSTALL) -c $(INSTDATFLAGS) XErrorDB $(LIBDIR)

