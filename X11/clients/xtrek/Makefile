	$(MKDIRHIER) $(XTREKLIBDIR)

install:: default.config
	$(INSTALL) -c $(INSTDATFLAGS) default.config $(XTREKLIBDIR)

$(XTREKFONT).snf:  $(XTREKFONT).bdf
	$(FONTC) $(FONTCFLAGS) $? >$@

