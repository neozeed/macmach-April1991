	$(INSTALL) -c $(INSTMANFLAGS) xtrekd.man $(MANDIR)/xtrekd.x

install:: $(XTREKD) $(XTREK)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(XTREKD) $(XTREK); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(XTREKBINDIR)); \
	done

