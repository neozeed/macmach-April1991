install.man:: $(MANPAGES)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(MANPAGES); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(LIBMANDIR)); \
	done

